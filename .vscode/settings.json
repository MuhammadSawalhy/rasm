{
   "liveServer.settings.port": 5502,
   /// live sass compiler.......
   "liveSassCompile.settings.excludeList": [
      "**/node_modules/**",
      "**/.vs/**",
      "**/libraries/**",
   ],
   "liveSassCompile.settings.formats": [
      {
         "format": "compressed",
         "extensionName": ".min.css",
         "savePath": "/dist/styles"
      }
   ],
   "liveSassCompile.settings.autoprefix": [
      "last 3 versions",
      "> 1%",
      "IE > 10"
   ],

}